<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="369dp"
        android:layout_height="63dp"
        android:text="@string/snow_prediction"
        android:textAlignment="center"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.058" />

    <TextView
        android:id="@+id/resultText"
        android:layout_width="249dp"
        android:layout_height="126dp"
        android:text="@string/answer"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.049" />

    <Button
        android:id="@+id/london"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:text="London"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/vienna"
        app:layout_constraintEnd_toStartOf="@+id/berlin"
        app:layout_constraintHorizontal_bias="0.42"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resultText" />

    <Button
        android:id="@+id/berlin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="3dp"
        android:insetRight="3dp"
        android:text="Berlin"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/york"
        app:layout_constraintEnd_toStartOf="@+id/moscow"
        app:layout_constraintStart_toEndOf="@id/london"
        app:layout_constraintTop_toBottomOf="@+id/resultText" />

    <Button
        android:id="@+id/moscow"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetRight="3dp"
        android:text="Moscow"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/milan"
        app:layout_constraintEnd_toStartOf="@id/dublin"
        app:layout_constraintStart_toEndOf="@id/berlin"
        app:layout_constraintTop_toBottomOf="@+id/resultText" />

    <Button
        android:id="@+id/dublin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="Dublin"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/minsk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/moscow"
        app:layout_constraintTop_toBottomOf="@+id/resultText" />

    <Button
        android:id="@+id/vienna"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:textSize="10sp"
        android:text="Vienna"
        app:layout_constraintBottom_toTopOf="@id/helsinki"
        app:layout_constraintEnd_toStartOf="@+id/york"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/london" />

    <Button
        android:id="@+id/york"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="3dp"
        android:insetRight="3dp"
        android:text="New York"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/kiev"
        app:layout_constraintEnd_toStartOf="@+id/milan"
        app:layout_constraintStart_toEndOf="@id/vienna"
        app:layout_constraintTop_toBottomOf="@+id/berlin" />

    <Button
        android:id="@+id/milan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetRight="3dp"
        android:text="Milan"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/beijing"
        app:layout_constraintEnd_toStartOf="@+id/minsk"
        app:layout_constraintStart_toEndOf="@id/york"
        app:layout_constraintTop_toBottomOf="@+id/moscow" />

    <Button
        android:id="@+id/minsk"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="Minsk"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/manukau"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/milan"
        app:layout_constraintTop_toBottomOf="@+id/dublin" />

    <Button
        android:id="@+id/helsinki"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:text="Helsinki"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/singapore"
        app:layout_constraintEnd_toStartOf="@+id/kiev"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vienna" />

    <Button
        android:id="@+id/kiev"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="3dp"
        android:insetRight="3dp"
        android:text="Kiev"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/tokyo"
        app:layout_constraintEnd_toStartOf="@id/beijing"
        app:layout_constraintStart_toEndOf="@id/helsinki"
        app:layout_constraintTop_toBottomOf="@+id/york" />

    <Button
        android:id="@+id/beijing"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetRight="3dp"
        android:text="Beijing"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/glasgow"
        app:layout_constraintEnd_toStartOf="@id/manukau"
        app:layout_constraintStart_toEndOf="@id/kiev"
        app:layout_constraintTop_toBottomOf="@+id/milan" />

    <Button
        android:id="@+id/manukau"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="Manukau"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/rome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/beijing"
        app:layout_constraintTop_toBottomOf="@+id/minsk" />


    <Button
        android:id="@+id/singapore"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:text="Singapore"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/cologne"
        app:layout_constraintEnd_toStartOf="@+id/tokyo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/helsinki" />

    <Button
        android:id="@+id/tokyo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="3dp"
        android:insetRight="3dp"
        android:text="Tokyo"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/belfast"
        app:layout_constraintEnd_toStartOf="@id/glasgow"
        app:layout_constraintStart_toEndOf="@id/singapore"
        app:layout_constraintTop_toBottomOf="@+id/kiev" />

    <Button
        android:id="@+id/glasgow"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetRight="3dp"
        android:text="Glasgow"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@id/paris"
        app:layout_constraintEnd_toStartOf="@id/rome"
        app:layout_constraintStart_toEndOf="@id/tokyo"
        app:layout_constraintTop_toBottomOf="@+id/beijing" />

    <Button
        android:id="@+id/rome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:textSize="10sp"
        android:text="Rome"
        app:layout_constraintBottom_toTopOf="@id/oslo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/glasgow"
        app:layout_constraintTop_toBottomOf="@+id/manukau" />

    <Button
        android:id="@+id/cologne"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:text="Cologne"

        android:textSize="10sp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/belfast"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/singapore" />

    <Button
        android:id="@+id/belfast"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="3dp"
        android:insetRight="3dp"
        android:text="Belfast"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/paris"
        app:layout_constraintStart_toEndOf="@id/cologne"
        app:layout_constraintTop_toBottomOf="@+id/tokyo" />

    <Button
        android:id="@+id/paris"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetRight="3dp"
        android:text="Paris"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/oslo"
        app:layout_constraintStart_toEndOf="@id/belfast"
        app:layout_constraintTop_toBottomOf="@+id/glasgow" />

    <Button
        android:id="@+id/oslo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="Oslo"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/paris"
        app:layout_constraintTop_toBottomOf="@+id/rome" />


</androidx.constraintlayout.widget.ConstraintLayout>